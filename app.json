{
  "expo": {
    "name": "Matka Office",
    "slug": "office",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "in.co.matkaoffice.officeapp",
      "supportsTablet": true,
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "in.co.matkaoffice.officeapp",
      "enableDangerousExperimentalLeanBuilds": true,
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
